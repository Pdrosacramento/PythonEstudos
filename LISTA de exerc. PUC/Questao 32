p=0
pc=0
mp=0
i=0
for n in range (1,11):
    n=int(input('Digite um numero'))
    if n<35:
        i=i+1
    if n>0:
        mp+=n
    if n>=50 and   n<=100:
        pc=pc+1
    if n<50 and n>=10 and n<=20:
        p=p+1
print(i,'Numeros inferiores a 35')    
print('A media dos numeros positivos Ã© :',mp/2)    
print('Porcentagem de numeros entre 50 e 100: ',pc*100/10)    
print('Porcentagem de numeros menores que 50 e/n entre 10 a 20 : ',p*100/10) 
    